# Say hi! #
(say hello!)
# => hello! #

# this is a multi
    line comment! #
(say (+ 1 2)) # => 3 #

(say (/ (* (+ 1 2) 3) 2))
# => 4.5 #

# Allocate a new variable, a #
(var a)
# Print out what a contains #
(say (val a))
# => None #

# Allocate a new variable, a #
(var a)
# Set a to 3 #
(set a 3)
# Print out a #
(say (val a))
# => 3 #

(var a)
(set a 1)
(var b)
(set b 2)
(say (+ (val a) (val b)))
# => 3 #

(set a 1)
(set b 2)
(say (> a b)) # => False #
(say (>= a b)) # => False #
(say (< a b)) # => True #
(say (<= a b)) # => True #
(say (== a b)) # => False #
